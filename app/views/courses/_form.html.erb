<%= render partial: 'shared/error', locals: { note: @course } %>



<%= form_for @course do |f| %>
<h2>Course </h2>
<%= f.label 'Course Name' %>
  <%= f.text_field :name%>
  <br>
  <%= f.label 'Course Subject' %>
  <%= f.text_field :subject%>
  <br>
  <%= f.label 'Course Teacher' %>
  <%= f.text_field :teacher%>
 <%= f.submit %>
<%end%>
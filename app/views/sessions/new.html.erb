<h1>Log In </h1>

<%= form_with(url: '/login') do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email %> <br>
  <%= f.label :password %>
  <%= f.password_field :password %><br>
  <%= f.submit "log in"%>
<% end %>

<%= link_to("Home", root_path) %>

<%= render partial: 'shared/flash', locals: { flash: flash } %>